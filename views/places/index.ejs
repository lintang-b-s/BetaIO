      <h1> Daftar tempat </h1>
      
          <% for(let place of places) { %>
              <div class="d-flex flex-row">
                  <div class="mb-3">
                      <div class="row">
                          <div class="col-md-4">
                              <% if(place.images.length) { %>
                                  <img src="<%= place.images[0].url %>" alt="" class="img-fluid">
                              <% } else { %>
                                  <img src="https://images.unsplash.com/photo-1667573144362-c81933bcb6de?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxlZGl0b3JpYWwtZmVlZHw4fHx8ZW58MHx8fHw%3D&auto=format&fit=crop&w=500&q=60" alt="" class="img-fluid">
                              <% } %>
      
                          </div>
                          <div class="col-md-7">
                              <div class="card-body">
                                  <h5 class="card-title"><%= place.title %></h5>
                                  <p class="card-text"><%= place.description %></p>
                                  <p class="card-text">
                                      <small class="text-muted"><%= place.location %></small>
                                  </p>
                                  <a class="btn btn-warning" href="/places/<%= place._id %>"> lihat <%= place.title %></a>
      
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
              <% } %>
     
